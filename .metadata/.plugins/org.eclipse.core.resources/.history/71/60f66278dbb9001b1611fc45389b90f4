echo **************************************************************
echo Generate JAR
echo **************************************************************
mvn clean package -DskipTests
echo **************************************************************
echo BUILD AND PUSH Docker
echo **************************************************************
docker stop microdeposit
docker rm  microdeposit
echo **************************************************************
echo docker  build -t johansebcc/ms-deposit-v1 .
echo docker push  johansebcc/ms-deposit-v1
echo **************************************************************
echo run image 
docker run -p 8006:8006 --name microdeposit --network aforo255-test
echo -d johansebcc/ms-deposit-v1
echo *************************************************************
echo End Process
echo *************************************************************